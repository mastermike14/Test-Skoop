<div id="index">

<h1>New parent</h1>

<% form_for(@parent) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= label_tag :name, "Twitter account:" %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= label_tag :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', parents_path %>
</div>
